<script>
    var arrNegara = ["Malaysia","Inggris","Jerman","Belanda"];

    //Tampilan array ke 0
    alert("Nilai Indeks ke 0: "+arrNegara[0].toString());

    //Pemberian nilai pada array 0
    arrNegara[0] ="Indonesia";

    //Tampilan Array ke 0 
    alert("Nilai indeks ke 0 :"+arrNegara[0].toString());
</script>